{
  "name": "rhabdomancer-server",
  "version": "1.0.0",
  "description": "node server that runs rhabdomancer",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix && yarn run clean",
    "build": "tsc && yarn run client-build",
    "prestart": "yarn run build",
    "start": "node .",
    "clean": "rm -rf dist",
    "client-start": "react-app-rewired start",
    "client-build": "react-app-rewired build",
    "client-test": "react-app-rewired test"
  },
  "author": "Alex Kafer",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "express": "^4.17.1",
    "linked-list-typescript": "^1.0.15",
    "node-sass": "^4.13.0",
    "pug": "^2.0.4",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.15",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.2.0",
    "react-swipe": "^6.0.4",
    "redux": "^4.0.4",
    "socket.io": "^2.3.0",
    "swipe-js-iso": "^2.1.5",
    "three": "^0.110.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.5",
    "@types/socket.io": "^2.1.4",
    "copyfiles": "^2.1.1",
    "customize-cra": "^0.9.1",
    "react-app-rewired": "^2.1.5",
    "tslint": "^6.0.0-beta0",
    "typescript": "^3.6.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "presets": [
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}
