{
  "name": "lightshow-games-server",
  "version": "1.0.0",
  "description": "node server that runs the light show game server",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "cd admin && yarn install",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix && yarn clean",
    "build": "tsc && cd admin && yarn build",
    "prestart": "yarn build",
    "dev": "NODE_ENV=dev node -r esm .",
    "start": "NODE_ENV=dev PORT=443 sudo node -r esm .",
    "clean": "rm -rf dist"
  },
  "author": "Alex Kafer",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "lowdb": "^1.0.0",
    "socket.io": "^2.3.0",
    "three": "^0.110.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "xmlhttprequest-ts": "^1.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^12.12.5",
    "@types/socket.io": "^2.1.4",
    "@types/uuid": "^3.4.6",
    "tslint": "^6.0.0-beta0",
    "typescript": "^3.6.4"
  }
}
