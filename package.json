{
  "name": "lightshow-games-server",
  "version": "1.0.0",
  "description": "node server that runs the light show game server",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix && yarn run clean",
    "build": "tsc",
    "prestart": "yarn run build",
    "start": "node .",
    "clean": "rm -rf dist"
  },
  "author": "Alex Kafer",
  "license": "ISC",
  "dependencies": {
    "@types/lowdb": "^1.0.9",
    "express": "^4.17.1",
    "lowdb": "^1.0.0",
    "socket.io": "^2.3.0",
    "three": "^0.110.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.5",
    "@types/socket.io": "^2.1.4",
    "tslint": "^6.0.0-beta0",
    "typescript": "^3.6.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "presets": [
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}
