{
  "name": "rhabdomancer-server",
  "version": "1.0.0",
  "description": "node server that runs rhabdomancer",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix && yarn run clean",
    "build": "tsc && yarn run client-build",
    "prestart": "yarn run build",
    "start": "node .",
    "clean": "rm -rf dist",
    "client-start": "react-app-rewired start",
    "client-build": "react-app-rewired build",
    "client-test": "react-app-rewired test"
  },
  "author": "Alex Kafer",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "pug": "^2.0.4",
    "queue-typescript": "^1.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.2.0",
    "socket.io": "^2.3.0",
    "three": "^0.110.0"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.5",
    "@types/socket.io": "^2.1.4",
    "copyfiles": "^2.1.1",
    "react-app-rewired": "^2.1.5",
    "tslint": "^6.0.0-beta0",
    "typescript": "^3.6.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
