{
  "name": "lightshow-games-server",
  "version": "1.0.0",
  "description": "node server that runs the light show game server",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "cd admin && npm install",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix && npm run clean",
    "build": "tsc && cd admin && npm run build",
    "prestart": "npm run build",
    "start": "NODE_ENV=dev node -r esm .",
