    "get-pixels": "^3.3.2",
    "http-proxy-middleware": "^0.20.0",
    "lodash": "^4.17.15",
    "lowdb": "^1.0.0",
    "socket.io": "^2.3.0",
    "three": "^0.110.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "xmlhttprequest": "^1.8.0",
